            <div>
              <label for="filter-min-calories" class="filter-input-label">Calories</label>
          <div class="filter-input-element">
            <input type="text" name="filter-min-price" id="filter-min-calories" class="filter-input" placeholder="min">
          </div>
          <div class="filter-input-element">
            <input type="text" name="filter-max-calories" id="filter-max-calories" class="filter-input" placeholder="max">
          </div>
        </div>
            
            <!-- This is for the Min and the Max amount of Proteins  -->
            
            <div>
                 <label for="filter-min-Protein" class="filter-input-label">Protein</label>
          <div class="filter-input-element">
            <input type="text" name="filter-min-Protein" id="filter-min-Protein" class="filter-input" placeholder="min">
          </div>
          <div class="filter-input-element">
            <input type="text" name="filter-max-Protein" id="filter-max-Protein" class="filter-input" placeholder="max">
          </div>
        </div>

            <!-- This is for the Min and the Max amount of Carbs  -->

            
                <div>
                 <label for="filter-min-carbs" class="filter-input-label">Carbs</label>
          <div class="filter-input-element">
            <input type="text" name="filter-min-carbs" id="filter-min-carbs" class="filter-input" placeholder="min">
          </div>
          <div class="filter-input-element">
            <input type="text" name="filter-max-carbs" id="filter-max-carbs" class="filter-input" placeholder="max">
          </div>
        </div>

            <!-- This is for the Min and the Max amount of Fats  -->

        
                <div>
                 <label for="filter-min-fat" class="filter-input-label">Fats</label>
