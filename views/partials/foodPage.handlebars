            <input type="text" name="filter-min-fat" id="filter-min-fats" class="filter-input" placeholder="min">
          </div>

          <div class="filter-input-element">
            <input type="text" name="filter-max-fat" id="filter-max-fat" class="filter-input" placeholder="max">
          </div>
        </div>
            </fieldset>
